{
  "name": "npm-check",
  "version": "6.0.1",
  "description": "Check for outdated, incorrect, and unused dependencies.",
  "main": "lib",
  "type": "module",
  "engines": {
    "node": ">=v18.17.1"
  },
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "scripts": {
    "format": "prettier -w .",
    "lint": "xo ./lib/*.js",
    "test": "npm run lint && ./bin/cli.js || echo Exit Status: $?."
  },
  "xo": {
    "space": 4,
    "rules": {
      "no-warning-comments": [
        0
      ],
      "global-require": [
        0
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
  "bin": {
    "npm-check": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylang/npm-check.git"
  },
  "keywords": [
    "npm",
    "outdated",
    "dependencies",
    "unused",
    "changelog",
    "check",
    "updates",
    "api",
    "interactive",
    "cli",
    "safe",
    "updating",
    "updater",
    "installer",
    "devDependencies"
  ],
  "author": {
    "name": "Dylan Greene",
    "email": "dylang@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dylang/npm-check/issues"
  },
  "homepage": "https://github.com/dylang/npm-check",
  "files": [
    "bin",
    "lib",
    "lib-es5"
  ],
  "dependencies": {
    "callsite-record": "^4.1.5",
    "chalk": "^5.3.0",
    "co": "^4.6.0",
    "depcheck": "^1.4.3",
    "execa": "^7.2.0",
    "giturl": "^2.0.0",
    "global-modules": "^2.0.0",
    "globby": "^13.2.2",
    "inquirer": "^9.2.10",
    "is-ci": "^3.0.1",
    "lodash": "^4.17.21",
    "meow": "^12.0.1",
    "minimatch": "^9.0.3",
    "node-emoji": "^2.1.0",
    "ora": "^7.0.1",
    "package-json": "^8.1.1",
    "path-exists": "^5.0.0",
    "pkg-dir": "^7.0.0",
    "preferred-pm": "^3.0.3",
    "rc-config-loader": "^4.1.3",
    "semver": "^7.5.4",
    "semver-diff": "^4.0.0",
    "strip-ansi": "^7.1.0",
    "text-table": "^0.2.0",
    "throat": "^6.0.2",
    "update-notifier": "^6.0.2",
    "xtend": "^4.0.2"
  },
  "devDependencies": {
    "prettier": "^3.0.1",
    "xo": "^0.37.1"
  }
}
